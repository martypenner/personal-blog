{
	"name": "swyxkit",
	"private": true,
	"version": "0.0.1",
	"scripts": {
		"dev": "concurrently --kill-others 'sst bind vite dev' 'npm:sst:dev'",
		"build": "vite build",
		"package": "vite package",
		"preview": "vite preview",
		"check": "svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-check --tsconfig ./jsconfig.json --watch",
		"test": "playwright test",
		"lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --write --plugin-search-dir=. .",
		"prepare": "husky install",
		"sst:deploy": "sst deploy",
		"sst:dev": "sst dev"
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx}": "echo disabled for now && exit 0 || eslint --cache --fix",
		"*.{js,jsx,ts,tsx,css,scss,md,svelte}": [
			"prettier --write --plugin-search-dir=. ."
		]
	},
	"devDependencies": {
		"@playwright/test": "^1.25.2",
		"@sveltejs/adapter-auto": "^1.0.0-next.72",
		"@sveltejs/kit": "^1.0.0-next.480",
		"@tailwindcss/typography": "^0.5.7",
		"@typescript-eslint/eslint-plugin": "^5.36.2",
		"@typescript-eslint/parser": "^5.36.2",
		"autoprefixer": "^10.4.8",
		"aws-cdk-lib": "2.110.1",
		"concurrently": "^8.2.2",
		"constructs": "10.3.0",
		"eslint": "^8.23.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"gray-matter": "^4.0.3",
		"husky": "^8.0.1",
		"lint-staged": "^13.0.3",
		"mdsvex": "^0.10.6",
		"postcss": "^8.4.16",
		"postcss-load-config": "^4.0.1",
		"prettier": "~3.2.4",
		"prettier-plugin-tailwindcss": "^0.5.11",
		"rehype-autolink-headings": "^6.1.1",
		"rehype-format": "^4.0.1",
		"rehype-slug": "^5.0.1",
		"rehype-stringify": "^9.0.3",
		"remark-abbr": "^1.4.1",
		"remark-autolink-headings": "^7.0.1",
		"remark-frontmatter": "^4.0.1",
		"remark-github": "^11.2.4",
		"remark-parse": "^10.0.1",
		"remark-rehype": "^10.1.0",
		"remark-sectionize": "^1.1.1",
		"remark-slug": "^7.0.1",
		"remark-toc": "^8.0.1",
		"rss": "^1.2.2",
		"sst": "^2.39.10",
		"svelte": "^3.50.1",
		"svelte-check": "^2.9.0",
		"svelte-kit-sst": "^2.39.10",
		"svelte-preprocess": "^4.10.7",
		"tailwindcss": "^3.1.8",
		"typescript": "^4.8.3",
		"tslib": "^2.6.2",
		"unified": "^10.1.2",
		"vfile": "^5.3.5",
		"vfile-reporter": "^7.0.4"
	},
	"engines": {
		"node": ">=16.7"
	},
	"type": "module",
	"dependencies": {
		"date-fns": "^2.29.2",
		"node-fetch": "^3.2.10",
		"parse-link-header": "^2.0.0",
		"prism-themes": "^1.9.0",
		"shiki": "^0.11.1",
		"snarkdown": "^2.0.0"
	}
}
